<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<HBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.litit.timeleft.MainController"
      style="-fx-background-color: rgba(0, 100, 100, 0.2); -fx-background-radius: 10;">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label fx:id="welcomeText" textFill="white"/>
    <FontIcon iconLiteral="fas-cog" iconSize="20" iconColor="white" onMouseClicked="#settings"/>
    <FontIcon iconLiteral="fas-times" iconSize="20" iconColor="white" onMouseClicked="#closeApp"/>
</HBox>
